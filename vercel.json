// HYPE CURVE
function drawHypeCurve(){
  const canvas=document.getElementById('hypeCurve');
  if(!canvas)return;
  const ctx=canvas.getContext('2d');
  const W=canvas.parentElement.offsetWidth;
  const H=300;
  canvas.width=W*devicePixelRatio;
  canvas.height=H*devicePixelRatio;
  canvas.style.width=W+'px';
  canvas.style.height=H+'px';
  ctx.scale(devicePixelRatio,devicePixelRatio);

  const pad={l:60,r:40,t:30,b:50};
  const cw=W-pad.l-pad.r,ch=H-pad.t-pad.b;

  // Hype curve path
  ctx.beginPath();
  ctx.strokeStyle='#e8e8e5';
  ctx.lineWidth=2;
  const pts=[[0,0.15],[0.08,0.8],[0.15,1],[0.22,0.65],[0.35,0.25],[0.55,0.35],[0.75,0.55],[0.9,0.72],[1,0.78]];
  pts.forEach(([x,y],i)=>{
    const px=pad.l+x*cw, py=pad.t+(1-y)*ch;
    i===0?ctx.moveTo(px,py):ctx.lineTo(px,py);
  });
  ctx.stroke();

  // Labels
  ctx.fillStyle='#bbb';
  ctx.font='10px DM Mono, monospace';
  ctx.letterSpacing='0.1em';
  const phases=[[0.05,'Innovation','Trigger'],[0.15,'Peak of','Hype'],[0.28,'Trough of','Disillusionment'],[0.55,'Slope of','Enlightenment'],[0.88,'Plateau of','Productivity']];
  phases.forEach(([x,l1,l2])=>{
    const px=pad.l+x*cw;
    const yBase=H-20;
    ctx.fillStyle='#bbb';
    ctx.textAlign='center';
    ctx.fillText(l1,px,yBase);
    ctx.fillText(l2,px,yBase+12);
  });

  // Technologies
  const techs=[
    {label:'CV Damage',x:.88,y:.78,color:'#111',size:7},
    {label:'AI Underwriting',x:.85,y:.72,color:'#111',size:7},
    {label:'Claims Triage',x:.82,y:.76,color:'#111',size:6},
    {label:'Fraud GNNs',x:.72,y:.54,color:'#888',size:6},
    {label:'Parametric AI',x:.70,y:.52,color:'#888',size:6},
    {label:'LLM Submission',x:.68,y:.58,color:'#888',size:6},
    {label:'PQC Migration',x:.58,y:.38,color:'#888',size:5},
    {label:'Digital Twin',x:.55,y:.34,color:'#aaa',size:5},
    {label:'Wearable UW',x:.78,y:.62,color:'#888',size:6},
    {label:'Cat Model ML',x:.65,y:.50,color:'#888',size:6},
    {label:'Blockchain Claims',x:.25,y:.26,color:'#e0e0de',size:5},
    {label:'AI Replaces UW',x:.22,y:.67,color:'#e0e0de',size:5},
    {label:'Actuarial LLMs',x:.45,y:.28,color:'#aaa',size:5},
    {label:'Agentic Brokers',x:.40,y:.25,color:'#ccc',size:5},
  ];

  techs.forEach(t=>{
    const px=pad.l+t.x*cw;
    const pIdx=Math.min(Math.floor(t.x*8),pts.length-1);
    const p1=pts[pIdx],p2=pts[Math.min(pIdx+1,pts.length-1)];
    const frac=(t.x-p1[0])/(p2[0]-p1[0]||1);
    const y=p1[1]+(p2[1]-p1[1])*frac;
    const py=pad.t+(1-y)*ch;

    ctx.beginPath();
    ctx.arc(px,py,t.size,0,Math.PI*2);
    ctx.fillStyle=t.color;
    ctx.fill();
    if(t.color==='#111'||t.color==='#888'){
      ctx.strokeStyle='rgba(255,255,255,.8)';
      ctx.lineWidth=1.5;
      ctx.stroke();
    }

    ctx.fillStyle='#555';
    ctx.font='bold 10px DM Sans, sans-serif';
    ctx.textAlign=px<W/2?'left':'right';
    const lx=px<W/2?px+t.size+4:px-t.size-4;
    ctx.fillText(t.label,lx,py+4);
  });

  // Y axis
  ctx.fillStyle='#ccc';
  ctx.font='10px DM Mono, monospace';
  ctx.textAlign='right';
  ctx.fillText('Expectations',pad.l-8,pad.t+ch/2);
}


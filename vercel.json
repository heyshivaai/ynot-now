// MINDS GRID
function renderMindsGrid(){
  document.getElementById('mindsGrid').innerHTML=MINDS.map(m=>`
    <div class="mc" id="mc-${m.id}">
      <span class="mc-icon">${m.icon}</span>
      <div class="mc-name">${m.name}</div>
      <div class="mc-role">${m.role}</div>
      <div class="mc-desc">${m.desc}</div>
      <div class="mc-domain">${m.domain}</div>
      <div class="mc-status"><div class="mcdot" id="dot-${m.id}"></div><span id="st-${m.id}">Warming up‚Ä¶</span></div>
      <div class="mc-bar"><div class="mc-fill" id="fill-${m.id}"></div></div>
    </div>`).join('');
}

function setMind(id,state,label){
  const card=document.getElementById('mc-'+id);
  const dot=document.getElementById('dot-'+id);
  const st=document.getElementById('st-'+id);
  const fill=document.getElementById('fill-'+id);
  if(!card)return;
  card.className='mc '+state;
  if(dot)dot.className='mcdot'+(state==='thinking'?' thinking':state==='done'?' done':'');
  if(st)st.textContent=label||state;
  if(fill){
    if(state==='thinking'){fill.style.animation='barSlide 1.7s ease-in-out infinite';fill.style.width='40%';}
    else if(state==='done'){fill.style.animation='none';fill.style.width='100%';}
    else{fill.style.animation='none';fill.style.width='0';}
  }
}

async function init(){
  renderMindsGrid();
  MINDS.forEach(m=>setMind(m.id,'idle','Warming up‚Ä¶'));
  // Load from weekly database run first
  try{
    const res=await fetch('/api/findings');
    if(res.ok){
      const data=await res.json();
      if(data.findings&&data.findings.length>0){
        insights=data.findings.map(f=>({
          id:f.id,mindId:f.mind_id,mindName:f.mind_name,mindIcon:f.mind_icon,
          title:f.title,verdict:f.verdict,body:f.body,domain:f.domain,
          subdomain:f.subdomain,confidence:f.confidence,trl:f.trl,
          regulatoryRisk:f.regulatory_risk,experiment:f.experiment,refs:f.refs||[],
        }));
        MINDS.forEach(m=>{
          const s=insights.filter(i=>i.mindId===m.id&&i.verdict==='SIGNAL').length;
          const tot=insights.filter(i=>i.mindId===m.id).length;
          if(tot>0)setMind(m.id,'done',s+' signal'+(s!==1?'s':'')+' found');
          else setMind(m.id,'idle','Next run scheduled');
        });
        if(data.trajectory&&data.trajectory.length>0)renderTrajectory(data.trajectory);
        const rEl=document.getElementById('cbRuns');if(rEl&&data.total_runs)rEl.textContent=data.total_runs;
        const eEl=document.getElementById('cbEver');if(eEl&&data.total_ever)eEl.textContent=data.total_ever;
        if(data.run_date){
          const d=new Date(data.run_date);
          document.getElementById('footerMeta').textContent=
            insights.length+' insights ¬∑ Last explored '+d.toLocaleDateString('en-US',{month:'short',day:'numeric',year:'numeric'});
        }
        document.getElementById('labLog').classList.remove('on');
        refresh();
        return;
      }
    }
  }catch(e){console.log('DB fallback:',e.message);}
  // Fallback: no DB yet ‚Äî run minds live
  setTimeout(()=>startMinds(),700);
}

function renderTrajectory(trajectory){
  const byTech={};
  trajectory.forEach(t=>{if(!byTech[t.technology_name])byTech[t.technology_name]=[];byTech[t.technology_name].push(t);});
  const movers=Object.entries(byTech)
    .filter(([,h])=>h.length>1)
    .map(([name,h])=>({name,first:h[0].trl,last:h[h.length-1].trl,delta:h[h.length-1].trl-h[0].trl,domain:h[h.length-1].domain}))
    .filter(m=>m.delta!==0).sort((a,b)=>Math.abs(b.delta)-Math.abs(a.delta)).slice(0,6);
  if(!movers.length)return;
  const wrap=document.getElementById('trajectoryWrap');
  if(!wrap)return;
  wrap.style.display='block';
  wrap.innerHTML='<div class="eyebrow" style="margin-bottom:16px;">How the landscape is shifting</div>'+
    '<div style="display:flex;flex-wrap:wrap;gap:10px;">'+
    movers.map(m=>{
      const up=m.delta>0;
      return `<div style="background:var(--white);border:1.5px solid var(--rule);border-radius:10px;padding:12px 16px;display:flex;align-items:center;gap:10px;">
        <span style="font-size:18px;color:${up?'#111':'#bbb'};font-weight:700;">${up?'‚Üë':'‚Üì'}${Math.abs(m.delta)}</span>
        <div><div style="font-size:13px;font-weight:500;">${m.name}</div>
        <div style="font-family:var(--ff-mono);font-size:9px;color:var(--mid);font-weight:300;">TRL ${m.first} ‚Üí ${m.last} ¬∑ ${m.domain}</div></div></div>`;
    }).join('')+'</div>';
}

async function startMinds(){
  document.getElementById('labLog').classList.add('on');
  const toRun=MINDS.filter(m=>!insights.some(i=>i.mindId===m.id));
  if(!toRun.length){addLog(ts(),'System','All minds have current findings ‚Äî explore the Signal Lab','done');return;}
  await Promise.allSettled(toRun.map(m=>runMind(m)));
  addLog(ts(),'System','All minds reporting ‚Äî '+insights.length+' findings scored','done');
  document.getElementById('footerMeta').textContent=insights.length+' insights ¬∑ Updated '+new Date().toLocaleTimeString();
}

async function runMind(mind){
  setMind(mind.id,'thinking','Exploring‚Ä¶');
  addLog(ts(),mind.name,'Examining '+mind.domain+' landscape‚Ä¶','');
  try{
    const res=await fetch('/api/think',{
      method:'POST',headers:{'Content-Type':'application/json'},
      body:JSON.stringify({mindId:mind.id,system:'You are a specialist AI insurance intelligence mind. Respond ONLY with a valid JSON array. No markdown fences, no preamble. Start with [ end with ]. Each item MUST include a refs array with 1-3 objects having label and url fields pointing to real, verifiable sources (arXiv, NAIC, FCA, EIOPA, academic institutions, established research orgs). Only use sources that genuinely exist.',prompt:mind.prompt})
    });
    const data=await res.json();
    const raw=data.content?.map(c=>c.text||'').join('')||'';
    let parsed;
    try{parsed=JSON.parse(raw.replace(/```json|```/g,'').trim());}
    catch{const m=raw.match(/\[[\s\S]*\]/);parsed=m?JSON.parse(m[0]):getFallback(mind.id);}
    parsed.forEach((ins,i)=>{
      ins.mindId=mind.id;ins.mindName=mind.name;ins.mindIcon=mind.icon;
      ins.id=`${mind.id}-${Date.now()}-${i}`;
      if(!ins.refs||!ins.refs.length)ins.refs=mind.refs;
    });
    insights.push(...parsed);
    const s=parsed.filter(x=>x.verdict==='SIGNAL').length;
    setMind(mind.id,'done',s+' signal'+(s!==1?'s':'')+' found');
    addLog(ts(),mind.name,parsed.length+' findings ‚Äî '+s+' confirmed signal'+(s!==1?'s':''),'done');
    refresh();
  }catch(err){
    const fb=getFallback(mind.id);
    fb.forEach((ins,i)=>{ins.mindId=mind.id;ins.mindName=mind.name;ins.mindIcon=mind.icon;ins.id=`${mind.id}-fb-${i}`;if(!ins.refs)ins.refs=mind.refs;});
    insights.push(...fb);
    const s=fb.filter(x=>x.verdict==='SIGNAL').length;
    setMind(mind.id,'done',s+' signals (curated)');
    addLog(ts(),mind.name,'Curated intelligence loaded ‚Äî '+s+' signals','done');
    refresh();
  }
}

function refresh(){
  const s=insights.filter(i=>i.verdict==='SIGNAL').length;
  const w=insights.filter(i=>i.verdict==='WATCH').length;
  const n=insights.filter(i=>i.verdict==='NOISE').length;
  const t=insights.length;
  document.getElementById('cbTotal').textContent=t;
  document.getElementById('cbSignal').textContent=s;
  if(t>0){
    document.getElementById('vrow').classList.add('on');
    document.getElementById('vbar').classList.add('on');
    document.getElementById('vns').textContent=s;
    document.getElementById('vnw').textContent=w;
    document.getElementById('vnn').textContent=n;
    document.getElementById('vbs').style.width=(s/t*100)+'%';
    document.getElementById('vbw').style.width=(w/t*100)+'%';
    const depth=Math.min(t/24*100,100);
    document.getElementById('cbFill').style.width=depth+'%';
    updateScore(s,t);
  }
  renderCards();
}

function updateScore(s,t){
  const ev=Math.round(insights.reduce((a,i)=>a+(i.confidence||3),0)/t*20);
  const trl=Math.round(insights.reduce((a,i)=>a+(i.trl||5),0)/t/9*100);
  const cr=Math.round(s/t*100);
  const rr=Math.round(insights.filter(i=>i.regulatoryRisk==='high').length/t*100);
  const score=Math.round(ev*.35+trl*.3+cr*.25+(100-rr)*.1);
  document.getElementById('scoreWrap').classList.add('on');
  document.getElementById('scoreBig').textContent=score;
  [['sbEv','svEv',ev],['sbTrl','svTrl',trl],['sbCr','svCr',cr],['sbRr','svRr',rr]].forEach(([b,v,val])=>{
    const be=document.getElementById(b);if(be)be.style.width=val+'%';
    const ve=document.getElementById(v);if(ve)ve.textContent=val;
  });
}

function renderCards(){
  const g=document.getElementById('cardsGrid');
  const f=insights.filter(i=>(domF==='all'||i.domain===domF)&&(verdF==='all'||i.verdict===verdF));
  if(!f.length){
    g.innerHTML=insights.length===0?'<div class="empty-state"><div class="es-q">Y?</div><div class="es-title">The minds are exploring.</div><div class="es-sub">Their findings appear here as they work through each domain.</div></div>':'<div class="empty-state"><div class="es-q">\u2205</div><div class="es-title">No matches.</div><div class="es-sub">Try adjusting the filters.</div></div>';
    return;
  }
  const cm={SIGNAL:'cs',NOISE:'cn',WATCH:''};
  const vm={SIGNAL:'vcs',NOISE:'vcn',WATCH:'vcw'};
  g.innerHTML=f.map((ins,idx)=>{
    const pips=Array.from({length:5},(_,i)=>`<div class="pip ${i<(ins.confidence||3)?'on':''}" ></div>`).join('');
    const safe=encodeURIComponent(JSON.stringify(ins));
    return `<div class="icard ${cm[ins.verdict]||''}" style="animation-delay:${idx*.04}s" onclick="openModal('${safe}')"><div class="ic-top"><div class="vc ${vm[ins.verdict]||''}">${ins.verdict}</div><div class="ic-sub">${ins.subdomain||ins.domain||''}</div></div><div class="ic-title">${ins.title}</div><div class="ic-body">${ins.body}</div><div class="ic-foot"><div class="ic-agent">${ins.mindIcon||''} ${ins.mindName}</div><div class="ic-meta"><div class="ic-trl">TRL ${ins.trl||'?'}</div><div class="pips">${pips}</div></div></div></div>`;
  }).join('');
}

function setDom(v,btn){domF=v;document.querySelectorAll('#domPills .pill').forEach(b=>b.classList.remove('on'));btn.classList.add('on');renderCards();}
function setVerd(v,btn){verdF=v;document.querySelectorAll('#verdPills .pill').forEach(b=>b.classList.remove('on'));btn.classList.add('on');renderCards();}

